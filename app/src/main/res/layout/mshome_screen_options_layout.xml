<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="@dimen/sd4"
    android:layout_weight="1"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"

        android:id="@+id/ms_myprofile"
        android:padding="@dimen/sd16"
        android:gravity="center"
        android:background="@drawable/newcurvedbg"
        android:layout_margin="4dp"
        android:orientation="vertical"
        >
        <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:padding="@dimen/sd4"
            android:src="@drawable/barber"
            />


    </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sd12"
            android:gravity="center"
            android:padding="@dimen/sd4"
            android:textStyle="bold"
            android:textColor="@color/darkgreen"
            android:text="My Profile"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/ms_home_beauticians"
            android:padding="@dimen/sd14"
            android:gravity="center"
            android:background="@drawable/newcurvedbg"
            android:layout_margin="4dp"
            android:orientation="vertical"
            >
        <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:padding="@dimen/sd4"
            android:src="@drawable/makeup"
            />
        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sd12"
            android:gravity="center"
            android:textStyle="bold"
            android:textColor="@color/darkgreen"
            android:padding="@dimen/sd4"
            android:text="Beauticians"
            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/ms_home_customers"
            android:padding="@dimen/sd14"
            android:gravity="center"
            android:background="@drawable/newcurvedbg"
            android:layout_margin="4dp"
            android:orientation="vertical"
            >
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:padding="@dimen/sd8"
                android:src="@drawable/ic_skincare2"
                />
        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sd12"
            android:gravity="center"
            android:padding="@dimen/sd4"
            android:textStyle="bold"
            android:textColor="@color/darkgreen"
            android:text="Customers"
            />

    </LinearLayout>
</LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:padding="@dimen/sd2"
        android:orientation="horizontal"
        >


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/sd4"
        android:orientation="horizontal"
        >
        <LinearLayout
            android:id="@+id/ms_home_invoice"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:padding="@dimen/sd18"
                android:background="@drawable/newcurvedbg"
                android:layout_margin="4dp"
                android:orientation="vertical"
                >
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:padding="@dimen/sd4"
                android:src="@drawable/budgeting"
                />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sd12"
                android:gravity="center"
                android:padding="@dimen/sd4"
                android:textStyle="bold"
                android:textColor="@color/darkgreen"
                android:text="Invoice"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/ms_home_services"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/sd14"
                android:gravity="center"
                android:background="@drawable/newcurvedbg"
                android:layout_margin="4dp"
                android:orientation="vertical"
                >
            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:padding="@dimen/sd4"
                android:src="@drawable/beautician"
                />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sd12"
                android:padding="@dimen/sd4"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/darkgreen"
                android:text="Services"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:id="@+id/ms_home_calendar"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="@dimen/sd14"
                android:background="@drawable/newcurvedbg"
                android:layout_margin="4dp"
                android:orientation="vertical"
                >
                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_weight="1"
                    android:padding="@dimen/sd4"
                    android:src="@drawable/calendar"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sd12"
                android:gravity="center"
                android:padding="@dimen/sd4"
                android:textStyle="bold"
                android:textColor="@color/darkgreen"
                android:text="Calendar"
                />

        </LinearLayout>
    </LinearLayout>
</LinearLayout>