                    android:inputType="textCapCharacters"
                    android:maxLines="1"
                    android:minHeight="45dp"
                    android:padding="5dp"
                    android:textColor="@color/darkgreen"
                    android:textSize="14sp"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/sd6"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/dis_adharPic"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/sd4"
                        android:layout_weight="1"
                        android:background="@drawable/cardbg"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="@dimen/sd8">

                        <ImageView

                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_marginHorizontal="@dimen/sd6"
                            android:src="@drawable/ic_baseline_document_scanner_24" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="adhar image"
                            android:textColor="@color/darkgreen" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/dis_panPic"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/sd4"
                        android:layout_weight="1"
                        android:background="@drawable/cardbg"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="@dimen/sd8">

                        <ImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"

                            android:layout_marginHorizontal="@dimen/sd6"

                            android:src="@drawable/ic_baseline_document_scanner_24" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="pan image"
                            android:textColor="@color/darkgreen" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>
        </RelativeLayout>
    </ScrollView>

    <Button
        android:id="@+id/dist_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/sd10"
        android:backgroundTint="@color/midgreen"
        android:padding="@dimen/sd4"
        android:visibility="gone"
        android:text="Update Profile"
        android:textColor="@color/darkgreen"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/customerOrderHistoryRecycler"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginHorizontal="@dimen/sd4"
        android:layout_marginVertical="@dimen/sd6"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="bottom"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/dist_customercarebutton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/sd14"
            android:layout_marginVertical="@dimen/sd8"
            android:layout_weight="1"
            android:backgroundTint="@color/midgreen"
            app:cardCornerRadius="@dimen/sd12">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:drawableEnd="@drawable/ic_supportdarkgreen"
                android:gravity="center"
                android:padding="@dimen/sd8"
                android:text="Customer Care"
                android:textColor="@color/darkgreen"
                android:textSize="@dimen/sd10"
                android:textStyle="bold" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/dist_privacypolicyButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/sd14"
            android:layout_marginVertical="@dimen/sd8"
            android:layout_weight="1"
            android:backgroundTint="@color/midgreen"
            app:cardCornerRadius="@dimen/sd12">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:drawableEnd="@drawable/ic_baseline_text_snippet_24"
                android:gravity="center"
                android:padding="@dimen/sd8"
                android:text="Privacy Policy"
                android:textColor="@color/darkgreen"
                android:textSize="@dimen/sd10"
                android:textStyle="bold" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/sd14"
            android:layout_marginVertical="@dimen/sd8"
            android:layout_weight="1"
            android:backgroundTint="@color/midgreen"
            app:cardCornerRadius="@dimen/sd12">

            <TextView
                android:id="@+id/dist_Logout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:drawableEnd="@drawable/ic_logout_darkgreen"
                android:gravity="center"
                android:padding="@dimen/sd8"
                android:text="Logout"
                android:textColor="@color/lightred"
                android:textSize="@dimen/sd10"
                android:textStyle="bold" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>




